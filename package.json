{
  "name": "otc-desk",
  "version": "0.0.1",
  "private": true,
  "useWorkspaces": true,
  "main": "index.js",
  "workspaces": [
    "api",
    "contract",
    "_agstate/agoric-servers"
  ],
  "parsers": {
    "js": "mjs"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-jessie": "^0.0.6",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "^1.18.2",
    "@jessie.js/eslint-plugin": "^0.1.3",
    "@endo/eslint-plugin": "^0.3.12"
  },
  "globals": {
    "harden": "readonly"
  },
  "scripts": {
    "preinstall": "node -e \"process.env.AGORIC_INSTALL && process.exit(0); console.warn('please use: agoric install . For details, see https://agoric.com/documentation/'); process.exit(1)\"",
    "OFF-clean": "yarn workspaces run clean",
    "prettier": "yarn workspaces run prettier",
    "lint-fix": "yarn workspaces run lint-fix",
    "lint-check": "yarn workspaces run lint-check",
    "test": "yarn workspaces run test",
    "build": "yarn workspaces run build"
  },
  "dependencies": {
    "agoric": "*",
    "babel-eslint": ">=11.0.0-beta.2",
    "eslint-plugin-eslint-comments": "^3.1.2"
  },
  "resolutions": {
    "**/esm": "agoric-labs/esm#Agoric-built"
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  }
}
